<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.afoxplus.restaurants.demo">
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:name="com.afoxplus.restaurants.demo.DemoApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ThemeDemo">
        <activity
            android:name=".delivery.views.activities.MainActivity"
            android:exported="true"
            android:theme="@style/UIKit.NoActionBar.Light" />
        <activity
            android:name="com.afoxplus.module.delivery.views.activities.DemoConfigStartActivity"
            android:exported="true"
            android:theme="@style/UIKit.NoActionBar.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>